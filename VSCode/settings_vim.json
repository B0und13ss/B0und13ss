{
  "breadcrumbs.enabled": false,
  "chat.commandCenter.enabled": false,
  "code-runner.runInTerminal": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.preserveFocus": false,
  "code-runner.showExecutionMessage": false,
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "javascript": "node",
    "typescript": "ts-node",
    "python": "python3"
  },
  "C_Cpp_Runner.cStandard": "gnu23",
  "C_Cpp_Runner.cppStandard": "gnu++23",
  "debug.showInlineBreakpointCandidates": false,
  "editor.codeLens": false,
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "line-thin",
  "editor.cursorSurroundingLines": 10,
  "editor.fontFamily": "JetBrainsMonoNL NFM SemiBold", // "CaskaydiaMono NFM Light",
  "editor.fontLigatures": false,
  "editor.fontSize": 18,
  "editor.guides.indentation": false,
  "editor.lightbulb.enabled": "off",
  "editor.lineHeight": 1.625,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.showFoldingControls": "never",
  "editor.stickyScroll.enabled": false,
  "editor.suggest.insertMode": "insert",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.dialog.defaultPath": "C:\\Users\\ian\\Code\\",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.port": 8080,
  "vsicons.presets.hideExplorerArrows": true,
  "security.workspace.trust.untrustedFiles": "open",
  "security.allowedUNCHosts": ["wsl.localhost"],
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.focusAfterRun": "terminal",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.tabStopWidth": 2,
  "vsicons.dontShowNewVersionMessage": true,
  "window.commandCenter": false,
  "window.customMenuBarAltFocus": false,
  "window.density.editorTabHeight": "compact",
  "window.enableMenuBarMnemonics": false,
  "window.menuBarVisibility": "hidden",
  "window.newWindowProfile": "Default",
  "window.restoreWindows": "none",
  "window.title": "NeoCode",
  "workbench.colorTheme": "Vim Dark Hard",
  "workbench.panel.defaultLocation": "right",
  "workbench.activityBar.location": "hidden",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.panel.opensMaximized": "preserve",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 24,
  "workbench.tree.renderIndentGuides": "none",
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  // ! Prettier
  "prettier.printWidth": 180,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.bracketSpacing": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  // ! Android Emulator
  "emulator.hasEditorTitleIcon": false,
  "emulator.emulatorPath": "C:\\android-sdk\\emulator",
  "emulator.emulatorPathWindows": "C:\\android-sdk\\emulator",
  // ! Custom Appearances
  "workbench.colorCustomizations": {
    "scrollbar.shadow": "#00000000",
    "[Monokai Pro]": {
      "titleBar.border": "#00000000",
      "sideBar.border": "#00000000",
      "statusBar.border": "#00000000",
      "editorGroup.border": "#00000000",
      "panel.background": "#2d2a2e",
      "terminal.background": "#2d2a2e"
    },
    "[Catppuccin Mocha]": {
      "sideBar.background": "#11111b",
      "sideBarTitle.background": "#11111b",
      "sideBarSectionHeader.background": "#11111b",
      "statusBar.border": "#1e1e2e",
      "tab.activeBorderTop": "#1e1e2e",
      "tab.border": "#00000000",
      "tab.inactiveBackground": "#11111b",
      "editorGroup.border": "#1e1e2e",
      "titleBar.inactiveBackground": "#11111b",
      "panel.background": "#11111b",
      "statusBarItem.remoteBackground": "#f5c2e7",
      "statusBar.background": "#f5c2e7",
      "statusBar.noFolderBackground": "#f5c2e7",
      "statusBar.debuggingBackground": "#f5c2e7",
      "statusBarItem.remoteForeground": "#11111b",
      "statusBar.foreground": "#11111b",
      "statusBar.noFolderForeground": "#11111b",
      "statusBar.debuggingForeground": "#11111b"
    },
    "[Default Dark Modern]": {
      "titleBar.border": "#2b2b2b00",
      "titleBar.inactiveBackground": "#181818",
      "titleBar.inactiveForeground": "#cccccc",
      "tab.activeBorderTop": "#1f1f1f",
      "statusBar.noFolderBackground": "#181818",
      "statusBar.noFolderForeground": "#cccccc",
      "statusBar.debuggingBackground": "#181818",
      "statusBar.debuggingForeground": "#cccccc",
      "statusBarItem.remoteBackground": "#181818",
      "statusBarItem.remoteForeground": "#cccccc"
    },
    "[Vim Dark Hard]": {
      "titleBar.border": "#3c3836",
      "panel.border": "#3c3836",
      "statusBar.border": "#3c3836",
      "sideBar.border": "#3c3836",
      "statusBar.debuggingBackground": "#1d2021",
      "statusBar.debuggingForeground": "#ebdbb2",
      "statusBarItem.remoteBackground":"#1d2021",
    }
  },
  // ! VSCode Vim
  "vim.disableExtension": false,
  "vim.leader": "<Space>",
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.useCtrlKeys": true,
  "vim.changeWordIncludesWhitespace": true,
  "vim.smartRelativeLine": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgb(174, 175, 173)",
  "vim.highlightedyank.textColor": "rgb(31, 31, 31)",
  "vim.highlightedyank.duration": 100,
  "vim.normalModeKeyBindingsNonRecursive": [
    {"before": ["<Escape>"], "commands": [":noh"]},
    {"before": ["leader", "w"], "commands": [":w!"]},
    {"before": ["leader", "x"], "commands": [":x!"]},
    {"before": ["<S-h>"], "commands": [":bprevious"]},
    {"before": ["<S-l>"], "commands": [":bnext"]},
    {
      "before": ["leader", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["leader", "j"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["leader", "k"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["leader", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["leader", "/"],
      "commands": ["fuzzySearch.activeTextEditor"]
    },
    // ! f
    {
      "before": ["leader", "f", "r"],
      "commands": ["workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"]
    },
    {"before": ["leader", "f", "f"], "commands": ["editor.action.formatDocument"]},
    // ! g
    {
      "before": ["leader", "g", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["leader", "g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["leader", "g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["leader", "g", "I"],
      "commands": ["editor.action.goToImplimentation"]
    },
    // ! p
    {"before": ["leader", "p", "v"], "commands": [":vsplit"]},
    {"before": ["leader", "p", "h"], "commands": [":split"]},
    // ! r
    {
      "before": ["leader", "r", "s"],
      "commands": ["code-runner.stop"]
    },
    {
      "before": ["leader", "r", "r"],
      "commands": ["code-runner.run"]
    },
    {
      "before": ["leader", "r", "e"],
      "commands": ["emulator.start"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {"before": ["<C-h>"], "commands": ["cursorLeft"]},
    {"before": ["<C-l>"], "commands": ["cursorRight"]},
    {"before": ["<C-j>"], "commands": ["cursorDown"]},
    {"before": ["<C-k>"], "commands": ["cursorUp"]}
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {"before": ["<"], "commands": ["editor.action.outdentLines"]},
    {"before": [">"], "commands": ["editor.action.indentLines"]},
    {"before": ["J"], "commands": ["editor.action.moveLinesDownAction"]},
    {"before": ["K"], "commands": ["editor.action.moveLinesUpAction"]},
    {
      "before": ["leader", "c", "c"],
      "commands": ["editor.action.commentLine"]
    }
  ],
}
